{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [true, "attribute", "lib", "camelCase"],
    "component-selector": [false, "element", "lib", "kebab-case"],
    "curly": false,
    "forin": false,
    "interface-over-type-literal": false,
    "no-string-throw": false
  }
}
